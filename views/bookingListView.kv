#:kivy 1.0

<BookingListView>:
	GridLayout:
		cols: 1		
		Carousel:
			id: bookings
	GridLayout:
		cols: 4
		size_hint_y: 0.075		
		GridLayout:
			size_hint_x: 15.0
		Button:
			text: 'P'
			background_color: [0.5, 0.5, 0.5, 1.0]
			opacity: 0.5
			on_press: root.productsButtonClick(self)
		Button:
			text: 'T'
			background_color: [0.5, 0.5, 0.5, 1.0]
			opacity: 0.5
			on_press: root.timeslotsButtonClick(self)
		Button:
			text: 'P'
			background_color: [0.5, 0.5, 0.5, 1.0]
			opacity: 0.5
			on_press: root.peopleButtonClick(self)
				
				
				